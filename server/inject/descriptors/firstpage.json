{
  "type": "object",
  "properties": {
    "isFirstpage": {
      "type": "identifier",
      "selector": "div",
      "control": "exists",
      "selectContains": "Applications"
    },
    "devLink": {
      "type": "action",
      "selector": ".panel-text-title",
      "control": "property.innerText",
      "selectContains": "Development Management"
    }
  },
  "connect": [
    {
      "match": [
        "isFirstpage"
      ],
      "action": "devLink"
    }
  ]
}
